<!DOCTYPE html>
<html>
  <head>
    <title>Add Tourist</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <form  id="formAddTourist" name="addtourist" method="post" action="/addtourist">
      <input id="inputID" type="text" placeholder="TouristID" name="TouristID" />
      <input id="inputName" type="text" placeholder="Name" name="Name" />
      <input id="inputSurname" type="text" placeholder="Surname" name="Surname" />
      <input id="inputSex" type="text" placeholder="Sex" name="Sex" />
      <input id="inputCountry" type="text" placeholder="Country" name="Country" />
      <input id="inputNotes" type="text" placeholder="Notes" name="Notes" />
      <input id="inputDate" type="text" placeholder="Date of birth" name="Birthdate" />
      <input id="inputFlightIDs" type="text" placeholder="FlightIDs" name="FlightIDs" />
      <button id="btnSubmit" type="submit">Submit</button>
    </form>
  </body>
</html>